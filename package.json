  "version": "0.3.0",
  "description": "Table provider for Caldav (Calendar events)",
  "main": "index.js",
  "dependencies": {
    "@saltcorn/markup": "^1.0.0",
    "@saltcorn/data": "^1.0.0",
    "tsdav": "2.1.3",
    "cal-parser": "1.0.2",
    "node-fetch": "2.6.9"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "eslintConfig": {
    "extends": "eslint:recommended",
